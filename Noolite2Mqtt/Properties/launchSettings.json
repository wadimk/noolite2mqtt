{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "noolite2mqtt": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "v1/weatherforecast",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "http://localhost:5000"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/v1/weatherforecast",
      "publishAllPorts": true,
      "useSSL": false
    }
  }
}